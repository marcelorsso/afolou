import turtle 
import random

def color_size(ryannn, l):
    color = '444C38'
    if l <=8:
        color = '#00FF00' if random.uniform(0, 10) > 3.5 else '#FF69B4'
        elif 1 <= 30
        color = '#006600'
        ryannn.pencolor(color)
        ryannn.pensize(1/10)

        def angle():
            return random.uniform(10, 40)


        def new_lenght (l):
            return l + random.gauss(-5, 1)

        def tree(ryannn, l,tree_on_middle=False):
            if l <= 0: 
                return

                angle_r = angle()
                angle_l = angle()
                angle_m = (angle_l + angle_r) /2

                color_size(ryannn, l)
                ryannn.forward(l)

                ryannn.left(angle_l)
                tree(ryannn, new_lenght(l), tree_on_middle)

                ryannn.right(angle_m)
                if tree_on_middle:
                    tree(ryannn, new_lenght(l), tree_on_middle)

                    ryannn.right(angle_m)
                    tree(ryannn, new_lenght(l), tree_on_middle)

                    ryannn.left(angle_r)

                    color_size(ryannn, l)
                    ryannn.backward(l)

                    my_turtle = turtle.Turtle()
                    my_turtle.hideturtle()
                    my_turtle.speed(300)
                    my_turtle.left(90)
                    my_turtle.penup()
                    my_turtle.backward(200)
                    my_turtle.pendown()

                    tree(my_turtle, 40, False)

                    turtle.Screen().exitonclick()

